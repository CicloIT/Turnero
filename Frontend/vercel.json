{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/ver",
        "dest": "/build/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/build/index.html"
      }
    ]
  }
  